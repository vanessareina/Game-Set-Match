<h1>Edit this post</h1>

<%= form_for @post do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br />
  <%= f.label :preference %>
  <%= f.text_field :preference %><br />
  <%= f.label :level %>
  <%= f.text_field :level %><br />
  <%= f.label :age_group %>
  <%= f.text_field :age_group %><br />
  <%= f.label :city %>
  <%= f.text_field :city %><br />
  <%= f.label :state %>
  <%= f.text_field :state %><br />
  <%= f.label :date %>
  <%= f.text_field :date %><br />
  <%= f.label :time %>
  <%= f.text_field :time %><br />
  <%= f.label :comments %>
  <%= f.text_field :comments %><br />
  <%= f.submit "Update Post" %>
<% end %>
